{
  "name": "insight-flow-trading-platform",
  "version": "1.0.0",
  "description": "AI-powered trading intelligence platform with 10 core systems",
  "main": "next.config.js",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "setup": "node scripts/setup-env.js"
  },
  "dependencies": {
    "next": "14.2.30",
    "react": "^18",
    "react-dom": "^18",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.0",
    "tailwindcss": "^3.4.0",
    "lucide-react": "^0.294.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "eslint": "^8",
    "eslint-config-next": "14.2.30"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lovable": {
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "installCommand": "npm install",
    "framework": "nextjs",
    "nodeVersion": "18",
    "port": 3000,
    "environmentVariables": [
      "NEXT_PUBLIC_SUPABASE_URL",
      "NEXT_PUBLIC_SUPABASE_ANON_KEY",
      "SUPABASE_SERVICE_ROLE_KEY",
      "OPENAI_API_KEY",
      "STRIPE_SECRET_KEY",
      "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
      "NEXT_PUBLIC_APP_URL",
      "NEXT_PUBLIC_API_URL",
      "NEXT_PUBLIC_IS_LOVABLE"
    ],
    "features": {
      "ai": true,
      "database": true,
      "authentication": true,
      "payments": true,
      "real-time": true,
      "mobile": true
    }
  }
} 