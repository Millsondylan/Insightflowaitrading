    "android": "npm run build:capacitor && npx cap open android",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "i18n:extract": "i18next-scanner --config i18next-scanner.config.js",
    "cap:init": "cap init InsightFlow --web-dir=out",
    "cap:build": "npm run build && node scripts/prepare-capacitor.js && npx cap copy",
    "cap:android": "npx cap add android && npx cap open android",
    "cap:ios": "npx cap add ios && npx cap open ios",
    "cap:electron": "npx cap add @capacitor-community/electron && npx cap open @capacitor-community/electron",
    "cap:sync": "npx cap sync",
    "build:android": "npm run cap:build && npx cap sync android",
    "build:ios": "npm run cap:build && npx cap sync ios",
    "build:electron": "npm run cap:build && npx cap sync @capacitor-community/electron"
