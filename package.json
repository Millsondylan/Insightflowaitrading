    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "convert-to-lovable": "node scripts/convert-to-lovable.js",
    "lovable": "npx lovable-tagger serve src/lovable-demo/LovableDemo.tsx",
    "lovable:dev": "vite --config vite.lovable.config.ts",
    "lovable:build": "vite build --config vite.lovable.config.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "fix-lovable": "node --experimental-modules fix-lovable-compatibility.js && node --experimental-modules fix-jsx-elements.js && node --experimental-modules add-lovable-exports.js",
