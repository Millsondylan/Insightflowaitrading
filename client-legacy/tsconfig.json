{
  "extends": "../tsconfig.json",
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build", "dist", "**/*.test.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strict": false,
    "paths": {
      "@/*": ["./src/*"],
      "@shared/*": ["../shared/*"],
      "@assets/*": ["../attached_assets/*"]
    }
  }
} 