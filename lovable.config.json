{
  "project": {
    "name": "Insight Flow AI Trading",
    "description": "AI-powered trading strategy builder and portfolio management platform",
    "version": "2.0.0"
  },
  "visualEditing": {
    "enabled": true,
    "supportsTailwind": true,
    "componentExtension": ".lovable.tsx",
    "stableIdStrategy": "attribute-based"
  },
  "components": {
    "sourcePath": "src/components",
    "modules": "src/modules",
    "pages": "src/pages"
  },
  "integration": {
    "supabase": {
      "enabled": true
    },
    "github": {
      "enabled": true,
      "autoCommit": false
    },
    "nextjs": {
      "enabled": false,
      "migrationAvailable": true
    }
  },
  "paths": {
    "assets": "public",
    "styles": "src/styles",
    "config": "src/lib/config.ts"
  },
  "features": {
    "componentLibrary": true,
    "visualEditing": true,
    "aiGeneration": true,
    "themeSupport": true,
    "responsivePreview": true,
    "componentSearch": true,
    "propEditing": true,
    "exportToCode": true
  }
}
